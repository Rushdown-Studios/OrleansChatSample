{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "rpc-server-route": {
        "ClusterId": "rpc-server-cluster",
        "Match": {
          "Path": "/{**catchall}"
        }
      }
    },
    "Clusters": {
      "rpc-server-cluster": {
        "Destinations": {
          "rpc-1": {
            "Address": "ws://rpc-server-1:21111"
          },
          "rpc-2": {
            "Address": "ws://rpc-server-2:21112"
          }
        }
      }
    }
  }
}